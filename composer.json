{
    "name": "indatus/command-scheduler",
    "description": "Schedule your artisan commands within your application",
    "keywords": ["laravel"],
    "authors": [
        {
            "name": "Ben Kuhl",
            "email": "bkuhl@indatus.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "jlogsdon/cli": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*@dev",
        "mockery/mockery": "dev-master@dev",
        "orchestra/testbench": "2.1.*"
    },
    "autoload": {
        "psr-0": {
            "Indatus\\CommandScheduler\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "minimum-stability": "stable"
}
